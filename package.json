{
  "name": "bungo",
  "version": "0.0.150",
  "description": "i know it's my destiny",
  "repository": "git@github.com:jeremyBanks/bungo.git",
  "author": "Jeremy Banks <_@jeremy.ca>",
  "license": "MIT",
  "scripts": {
    "bungo": "tsc --noEmit && (cd src && rome run index.ts \"$@\")",
    "build": "tsc --noEmit && (cd src && rome compile index.ts --bundle | dd bs=1 skip=16 > ../dist/index.js)",
    "lint": "tsc --noEmit && (cd src && rome lint index.ts)",
    "full-bungo": "yarn lint; yarn build && echo && node dist/index.js \"$@\""
  },
  "dependencies": {
  },
  "devDependencies": {
    "prettier": "^2.0.5",
    "rome": "./node_modules/rome",
    "typescript": "^3.8.3"
  }
}
